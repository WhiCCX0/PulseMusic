<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <alpha
        android:duration="500"
        android:fromAlpha="0"
        android:interpolator="@android:interpolator/anticipate_overshoot"
        android:toAlpha="1" />

    <translate
        android:duration="500"
        android:fromYDelta="100%"
        android:interpolator="@android:interpolator/anticipate_overshoot"
        android:toXDelta="0" />

</set>